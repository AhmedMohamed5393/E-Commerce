<h4 class="text-center">Products finder</h4>
<div class="d-flex justify-content-center col-sm-12 col-xs-12 col-12 m-3">
    <ul class="list-group col-*-12">
      <% brands.forEach((brand) => { %>
        <li class="list-group-item d-flex justify-content-between
                   align-items-center col-*-4" id="branditem">
          <a href="/products/<%= brand %>/1" class="text-decoration-none">
            <%= brand %>
          </a>
        </li>
      <% }); %>
    </ul>
</div>