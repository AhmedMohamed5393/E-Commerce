<aside class="d-flex justify-content-start m-3
              col-xl-3 col-lg-3 col-md-4 col-sm-6 col-xs-8 col-10">
    <div class="m-3">
        <h4 class="text-center">Products finder</h4>
        <ul class="list-group">
          <% brands.forEach((brand) => { %>
            <li class="list-group-item d-flex justify-content-between
                       align-items-center col-*-4" id="branditem">
              <a href="/products/<%= brand %>/1" class="text-decoration-none">
                <%= brand %>
              </a>
            </li>
          <% }); %>
        </ul>
    </div>
</aside>